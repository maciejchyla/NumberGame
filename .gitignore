# Rzeczy specyficzne dla użytkownika
.idea/**/workspace.xml
.idea/**/zadania.xml
.idea/**/usage.statistics.xml
.idea/**/słowniki
.idea/**/półka

# AWS specyficzne dla użytkownika
.idea/**/aws.xml

# Wygenerowane pliki
